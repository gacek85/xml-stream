{
	"name": "gacek85/xml-stream",
	"description": "Event driven reader for large XML files",
	"authors": [
		{
			"name": "Maciej Garycki",
			"email": "maciekgarycki@gmail.com"
		}
	],
	
	"require": {
		"php": ">=5.5.9",
		"filp/whoops": "~2.1",
        "symfony/event-dispatcher": "~3.1.4"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8.0",
        "fzaninotto/Faker": "~1.6.0"
	},
    "config": {
        "bin-dir": "bin/"
    },
	"autoload": {
		"psr-4": {
			"Gacek85\\XML\\": "src/XML/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "Gacek85\\XML\\Test\\": "test/XML/Test/"
        }
    },
	"minimum-stability": "dev"
}
